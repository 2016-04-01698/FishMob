<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.fishmob">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.FishMOb">
        <activity android:name=".CallViewButton"></activity>
        <activity
            android:name=".FishSellerPage"
            android:label="@string/title_activity_fish_seller_page"
            android:theme="@style/Theme.FishMOb.NoActionBar" />
        <activity android:name=".RegistrationActivity" />
        <activity android:name=".LogInActivity" />
        <activity
            android:name=".ui.login.LoginActivity"
            android:label="@string/title_activity_login" />
        <activity android:name=".FishSellerUI" />
        <activity android:name=".homeActivityforFishKeeper" />
        <activity android:name=".RecyclerViewer" />
        <activity android:name=".CustomerUI" />
        <activity android:name=".dashboardActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>